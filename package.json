{
  "name": "@behackl/citation-js-extras",
  "version": "0.1.0",
  "description": "Preserve custom BibTeX fields through citation-js and render academic bibliographies with linked titles, badges, and more.",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "test": "vitest run",
    "test:watch": "vitest",
    "prepublishOnly": "tsc"
  },
  "keywords": [
    "citation-js",
    "bibtex",
    "bibliography",
    "csl",
    "custom-fields",
    "academic"
  ],
  "author": "Benjamin Hackl",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/behackl/citation-js-extra.git"
  },
  "bugs": {
    "url": "https://github.com/behackl/citation-js-extra/issues"
  },
  "homepage": "https://github.com/behackl/citation-js-extra#readme",
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=18"
  },
  "sideEffects": false,
  "packageManager": "pnpm@10.29.1",
  "peerDependencies": {
    "citation-js": ">=0.7.0"
  },
  "devDependencies": {
    "@types/node": "^25.2.3",
    "citation-js": "^0.7.22",
    "typescript": "^5.9.3",
    "vitest": "^3.1.4"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild"
    ]
  }
}
